	function formAccessories() {
		var form = document.forms["configurationForm"];
		if ( typeof(form) == "undefined" ) return new Array();
		var accessories = new Array();
		if ( typeof(form.accessories) != "undefined" ) {
			if ( typeof(form.accessories.length) == "undefined" ) {
				accessories.push(form.accessories);
			}
			else {
				for (var i=0; i < form.accessories.length; i++) {
					accessories.push(form.accessories[i]);
				}
			}
		}
		for (var i=0; i < form.elements.length; i++) {
			if ( form.elements[i].name.match(/^groupedAccessory.*/) && ( form.elements[i].value != "" ) ) {
				accessories.push(form.elements[i]);
			}
		}
		return accessories;
	}

	function computeTotalAccessoryPrice() {
		var totalPrice = 0;
		for (var i in accessoryMap ) {
			if ( accessoryMap[i].selected ) {
				totalPrice += accessoryMap[i].msrp;
			}
		}
		return totalPrice;
	}

	function priceString(msrp) {
		var thePrice = msrp;
		if(thePrice != "" && thePrice != "&#160;"){
			thePrice = "$" + thePrice;
		}
		else if(thePrice == "0"){
			thePrice = "$0";
		}
		else thePrice = "";
		return thePrice;
	}

	function findLocale(){
		var locale;
		if (here.currentSub == "Interior" || here.currentSub == "Sound") {
			locale = "int";
		} else {
			locale = "ext";
		}
		return locale;
	}

	function updateAccessory( accessoryId ) {
		if(isDownloading == 1 && bandwidth == "high" && vpInstalled == true) {
			document.getElementById('loadingWarning').style.visibility='visible';
			return;
		}
		var accessory = accessoryMap["" + accessoryId];
		var selectedCheckBoxImage = "checkBox" + accessoryId;
		var updateRequired = false;
		var locale = findLocale();
		var thePrice = defaultCurrencyFormat(accessory.msrp);
		here.accessoryImagePath = accessory.imageUrl;
		document.contextImage.src = accessory.imageUrl;
		here.accessoryDescription = accessory.description;
		here.accessoryName = accessory.name;
		here.accessoryPrice = accessory.msrp;
		here.accessoryDisclaimer = accessory.disclaimer;
		here.accessoryState = "description";
		document.getElementById("lyr1").innerHTML = accessory.description;
		document.getElementById("accessoryName").innerHTML = accessory.name;
		document.getElementById("accessoryPrice").innerHTML = thePrice;
		toggleAccessoryDisclaimerLink(accessory);

		if ( ! accessory.selected ) {
			var conflicts = findConflicts(accessory);
			if ( conflicts.length == 0 ) {
				accessory.selected = true;
				document.images[""+ selectedCheckBoxImage].src = "bys_images/checkbox_on.gif"
				updateRequired = true;
			}
			else {
				accessory.selected = false;
				document.images[""+ selectedCheckBoxImage].src = "bys_images/checkbox_off.gif"

				showConflictDiv(accessory, conflicts);
				return;
			}
		}
		else {
			accessory.selected = false;
			document.images[""+ selectedCheckBoxImage].src = "bys_images/checkbox_off.gif";
			updateRequired = true;
		}

		if ( updateRequired ) {
			updateAccessoryInfo();
		}
		accessoryChoice(accessory, accessory.viewpointKey, locale);
		initScrollLayer(98,'scrollbar');
	}
	
	function toggleAccessoryDisclaimerLink(accessory) {
		var displayLink = false;
		if(accessory == "off") {
			if(here.accessoryDisclaimer != "" && here.accessoryDisclaimer != null) {
				displayLink = true;
			}
		} else if(accessory.disclaimer != "" && accessory.disclaimer != null) {
			displayLink = true;
		}
		
		if(displayLink == true) {
			document.getElementById("accessoryDisclaimerLink").style.display = "block";
		} else {
			document.getElementById("accessoryDisclaimerLink").style.display = "none";
		}
	}
	
	function updateExclusiveAccessory( accessoryId ) {
		var accessory = accessoryMap["" + accessoryId];

		var updateRequired = false;
		if(accessoryId=="none"){
		updateAccessoryInfo();
		}else{
		var accessoryGroup = accessory.group;

		var conflicts = findConflicts(accessory);
		if ( conflicts.length == 0 ) {

			accessory.selected = true;
			if ( accessoryGroup.isExclusive ) {
				for (var i=0; i < accessoryGroup.accessoryList.length; i++) {
					var testAccessory = accessoryGroup.accessoryList[i];
					if ( testAccessory.id != accessory.id ) {
						testAccessory.selected = false;
						//testAccessory.checkBox.checked = false;
					}
				}
				updateRequired = true;
			}
		}
		else {
			for (var i=0; i < accessoryGroup.accessoryList.length; i++) {
				var testAccessory = accessoryGroup.accessoryList[i];
				if ( testAccessory.selected && ( testAccessory.id != accessory.id ) ) {
					//testAccessory.checkBox.checked = true;
				}
				else {
					//testAccessory.checkBox.checked = false;
				}
			}
			showConflictDiv(accessory, conflicts);
			return false;
		}

		if ( updateRequired ) {
			updateAccessoryInfo();
		}
		return true;
	}
	}

	function updateAccessoryInfo() {
		updateExcludes();
		vehicle.accessoriesPrice = computeTotalAccessoryPrice();
		var totalPrice = defaultCurrencyFormat(vehicle.totalPrice());
		document.getElementById("currentVehiclePrice").innerHTML = totalPrice;
		vehicle.update();
		document.images["price_change_indicator"].src = "bys_images/price_change_indicator.gif";		
		here.disclaimerOmniture = "sci: bys: xb disclaimer " + here.accessoryName;
	}

	function accessoryDisclaimer() {
		if(here.accessoryState == "description") {
			document.getElementById("accessoryDisclaimerLink").innerHTML = '<a href="javascript:;" onclick="accessoryDisclaimer();">description</a>';
			document.getElementById("lyr1").innerHTML = here.accessoryDisclaimer; 
			here.accessoryState = "disclaimer";
		} else {
			document.getElementById("accessoryDisclaimerLink").innerHTML = '<a href="javascript:;" onclick="omniture(here.disclaimerOmniture);accessoryDisclaimer();">disclaimer</a>';
			document.getElementById("lyr1").innerHTML = here.accessoryDescription; 
			here.accessoryState = "description";
		}
		initScrollLayer(98,'scrollbar');
	}

	function updateExcludes() {
		var exclude = excludeList();

		for (var x in accessoryMap) {
			var accessory = accessoryMap[x];
			if ( exclude[x] ) {
				accessory.excluded = true;
			}
			else {
				accessory.excluded = false;
			}
		}
	}

	function excludeList() {
		var excludeList = new Array();

		for (var x in accessoryMap) {
			var accessory = accessoryMap[x];
			if (accessory.selected) {
				for (var x=0; x < accessory.excludeIds.length; x++) {
					excludeList[accessory.excludeIds[x]] = true;
				}
			}
		}

		return excludeList;
	}

	function findConflicts(accessory) {

		var selectedAccessories = new Array();

		for (var i in accessoryMap) {
		var selectedAccessory = accessoryMap[i];
		if ( selectedAccessory.selected ) selectedAccessories.push(selectedAccessory);
		}

		var accessoryExcludes = accessory.excludeIds;
		var conflicts = new Array();

		for (var i=0; i < selectedAccessories.length; i++) {

			var conflict = false;
			var checkAccessory = accessoryMap["" + selectedAccessories[i].id];
			if ( arrayContains(accessoryExcludes, checkAccessory.id) ) {
				conflict = true;
			}
			else if ( arrayContains(checkAccessory.excludeIds, accessory.id) ) {
				conflict = true;
			}
			if ( conflict ) {
				conflicts.push(checkAccessory);
			}
		}
		return conflicts;
	}

	function resolveConflicts() {
		hideConflictDiv();
		deselectAccessories(conflictAccessories);
		selectAccessoryById(selectedAccessory.id);
	}

	function showConflictDiv( accessory, conflicts ) {
		var conflictOutputDiv = document.getElementById("conflictDiv");
		var conflictList = "<p><b>Are you sure you want to make this selection?<br><br>Doing so will reset the following selections:</b></p><p><b>";

		for (var i=0; i < conflicts.length; i++) {
			var conAccessory = conflicts[i];
			var name = conAccessory.name;
			if ( ( conAccessory.group.isExclusive ) && ( conAccessory.group.showName ) ) {
				name = conAccessory.group.name + ": " + name;
			}
			conflictList += "- " + name + "<br>";
		}

		conflictList += "<b></p>";
		conflictList += "<p><b><a href=\"javascript:resolveConflicts();\">Yes</a><br><a href=\"javascript:hideConflictDiv();\">Cancel</a></b></p></b>";
		conflictOutputDiv.innerHTML = conflictList;
		selectedAccessory = accessory;
		conflictAccessories = conflicts;
		conflictOutputDiv.style.visibility = "visible";
	}

	function hideConflictDiv(){
		var conflictDiv = document.getElementById("conflictDiv");
		conflictDiv.style.visibility = "hidden";
	}

	function deselectAccessories(accessoryList) {
		var locale = findLocale();
		for (var i=0; i < accessoryList.length; i++) {
			var accessory = accessoryList[i];
			if (accessory.group.isExclusive) {
				toggleRadioButtonImage(accessory.group.id, "none");
			} else {
				accessory.selected = false;
				document.images["checkBox" + accessory.id].src = "bys_images/checkbox_off.gif";
				here.currentAccessory = accessory;
				setTimeout('accessoryChoice(here.currentAccessory, here.currentAccessory.viewpointKey, findLocale())', 800);
			}
		}
		updateAccessoryInfo();
	}

	function selectNoneForGroup(accessoryGroup) {
		var groupElementName = "groupedAccessory" + accessoryGroup.id;
		var accessories = formAccessories();
		for (var i=0; i < accessories.length; i++) {
			
			if ( ( accessories[i].name == groupElementName ) && ( accessories[i].value == "" ) ) {
				accessories[i].checked = true;
				break;
			}
		}
	}

	function selectAccessoryById(accessoryId) {
		var accessory = accessoryMap["" + accessoryId];

		if(accessory.group.isExclusive){
			toggleRadioButtonImage(accessory.group.id, accessory.id);
		} else {
			updateAccessory(accessory.id);
		}

	}

	function addHiddenField(form, fieldName, fieldValue) {
		var hiddenField = document.createElement("input");
		hiddenField.name = fieldName;
		hiddenField.type = "hidden";
		form.appendChild(hiddenField);
		hiddenField.value = fieldValue;
	}

	function updateFormAccessories(form) {
		var formNodes = form.childNodes;
		for (var i=0; i < formNodes.length; i++) {
			if ( ( formNodes[i].nodeName.toLowerCase() == "input" ) && ( formNodes[i].name == "accessories" ) ) {
				form.removeChild(formNodes[i]);
				i--;
			}
		}
		for (var accessoryId in accessoryMap) {
			var accessory = accessoryMap[accessoryId];
			if ( accessory.selected ) {
				addHiddenField(form, "accessories", accessory.id);
			}
		}
	}

	function initAccessoriesDisplay() {
		var sessionAccessoryIds = [
			
		];
		for (var accessoryId in accessoryMap) {
			var accessory = accessoryMap[accessoryId];
			accessory.selected = false;
		}
		for (var i=0; i < sessionAccessoryIds.length; i++) {
			var accessory = accessoryMap[sessionAccessoryIds[i]];
			accessory.selected = true;

			if ( accessory.group.isExclusive ) {
				toggleRadioButtonImage(accessory.group.id, accessory.id);
			}
			else {
				document.images["checkBox" + accessory.id].src = checkBoxOnImage.src;
			}

		}

		updateAccessoryInfo();
	}


