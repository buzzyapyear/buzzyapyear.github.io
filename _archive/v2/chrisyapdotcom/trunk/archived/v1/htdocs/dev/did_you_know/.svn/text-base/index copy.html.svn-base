function submitAction(actionUrl, target, width, height) {
	//alert('Safari');
	var form = document.getElementById("configurationForm");
	if(!width) width="605";
	if(!height) height="700";
	form.action = actionUrl;
	if ( typeof(target) != "undefined" ) {
		form.target = target;
	} else {
		form.target = "_top";
	}
	updateFormAccessories(form);
	form.submit();
}

function switchVehicle(vehicleKey) {
	var form = document.getElementById("configurationForm");
	form.vehicleKey.value=vehicleKey;
	submitAction("/BysDev2/pub/switchVehicle.do");
}



