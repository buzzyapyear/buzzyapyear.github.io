function goBack() {
	var lastMain = here.lastMain;
	if(here.currentMain == "financingDisclaimer" || here.currentMain == "pricingDisclaimer") {
		for(i=0;i<here.mainNavHistory.length;i++) {
			if(here.mainNavHistory[i] != "financingDisclaimer" && here.mainNavHistory[i] != "pricingDisclaimer") {
				lastMain = here.mainNavHistory[i];
				break;
			}
		}
	}
	if(lastMain == "get_it") { loadGetIt(); return; }
	if(lastMain == "customize_it") { loadCustomizeIt(); return; }
	if(lastMain == "findDealers") { loadFindDealers(); return; }
	if(lastMain == "dealerMap") { loadDealerMap(); return; }
	if(lastMain == "saveAndCompare") { loadSaveAndCompare(); return; }
}

function hideContentAreas() {
	document.getElementById("contentArea").style.display="none";
	document.getElementById("getItArea").style.display="none";
	document.getElementById("pricingDisclaimerArea").style.display="none";
	document.getElementById("financingDisclaimerArea").style.display="none";
	document.getElementById("vehicleLayerStandard_features").style.display="none";
	document.getElementById("findDealersArea").style.display="none";
	document.getElementById("saveAndCompareArea").style.visibility="hidden";
	document.getElementById("errorArea").style.display="none";
}

function updateLocation(newMain,newSub,firstLoad) {
	if(newMain != "get_it" && newMain != "findDealers" && newMain != "pricingDisclaimer" && newMain != "financingDisclaimer" && newSub != "Standard_features"){
		/*
		if(firstLoad != true){
			if(isDownloading == 1 && bandwidth == "high" && vpInstalled == true) {
				document.getElementById('loadingWarning').style.visibility='visible';
				return;
			}
		}*/
	}
		if(bandwidth == "low" || firefoxMac == true){
			document.lowBandImage.src=document.lowBandImage.src;
		}
here.lastMain = here.currentMain;
	here.currentMain = newMain;
	updateMainNavHistory(newMain);
	if(here.currentSub == "none") {
		if(here.currentMain == "customize_it") {
			here.currentSub = here.lastSub;
		} else {
			here.currentSub = newSub;
		}
	} else {
		here.lastSub = here.currentSub;
		here.currentSub = newSub;
	}
	document.getElementById("scrollbar").style.visibility = "hidden";
	if(newSub == "Transmission" || newSub == "Colors" || newSub == "Standard_features"){
		document.getElementById("scrollbar2").style.visibility = "hidden";
	}else{
		document.getElementById("scrollbar2").style.visibility = "visible";
	}

	// trigger viewpoint if the section has changed
	if(newSub == "Interior" || newSub == "Sound") {
		if(here.vpArea == "exterior") {
			here.vpArea = "interior";
			triggerInterior();
		}
	} else if(newSub == "none") {
		//don't trigger viewpoint
	} else {
		if(here.vpArea == "interior") {
			here.vpArea = "exterior";
			triggerExterior();
		}
	}

	if(here.currentSub != "none") updateSubNavHistory(here.currentSub);
	didYouKnowUpdate();
	setTimeout("updateMainNavTabs()", 100); //timeout fixes IE image swapping bug

	if(here.currentSub != "none") {
	// accessory area tab and layer display updates
		document.getElementById("wholeWorkArea").style.visibility="visible";
		document.getElementById("wholeWorkArea").style.display="block";
		document.getElementById("lyr1").style.display="block";

		var layer = document.getElementById("vehicleLayer" + here.currentSub);
		var lastLayer = document.getElementById("vehicleLayer" + here.lastSub);
		if ( layer != null ) {
			layer.style.display="block";
		}
		if ( lastLayer != null && lastLayer.id != layer.id ) {
			lastLayer.style.display="none";
		}
		// turn off last tab
		if ( here.currentSub != here.lastSub ) {
			var oldTab = document.images[here.lastSub];
			oldTab.src = here.tabPath + here.lastSub + "_off.gif";
		}
		// change visited tab states
		for(var i in here.subNavHistory){
			document.images[here.subNavHistory[i]].src = here.tabPath + here.subNavHistory[i] + "_visited.gif";
		}
		// turn selected subnav tab to 'at' state
		if(document.images[here.currentSub]) {
			document.images[here.currentSub].src = here.tabPath + here.currentSub + "_at.gif";
		}
		clearInfoArea();
	}
}

function checkIfVisited(page) {
	for(i=0;i<here.mainNavHistory.length;i++) {
		if(page == here.mainNavHistory[i]) {
			return true;
		}
	}
	return false;
}

function updateMainNavHistory(page) {
	for(i=0;i<here.mainNavHistory.length;i++) {
		if(page == here.mainNavHistory[i]) {
			here.mainNavHistory.splice(i,1);
		}
	}
	here.mainNavHistory.unshift(page);
	if(here.mainNavHistory.length > 6) {
		here.mainNavHistory.pop();
	}
}

function clearInfoArea() {
	if (here.currentSub !="Colors" && here.currentSub != "Standard_features") {
		here.accessoryImagePath = "bys_images/accessory_images/default" + here.currentSub + ".jpg";
		here.accessoryName = "";
		document.getElementById("accessoryName").innerHTML = "";
	} else {
		var selectedColor = "";
		for (var i in colorMap) {
			var color = colorMap[i];
			if ( color.selected ) {
				selectedColor=color.id;
				here.accessoryImagePath = color.imageUrl;
				var colorName = color.colorName;
				here.accessoryName = colorName;
				document.getElementById("accessoryName").innerHTML = colorName;
				break;
			}
		}
	}
	here.accessoryDescription = "";
	here.accessoryPrice = "";
	here.accessoryDisclaimer = "";
	if(here.accessoryImagePath != "") document.contextImage.src = here.accessoryImagePath;
	document.getElementById("lyr1").innerHTML = "";
	document.getElementById("accessoryPrice").innerHTML = "";
	document.getElementById("accessoryDisclaimerLink").innerHTML = '<a href="javascript:;" onclick="omniture(here.disclaimerOmniture);accessoryDisclaimer();">disclaimer</a>';
	document.getElementById("accessoryDisclaimerLink").style.display = "none";
}

function updateMainNavTabs() {
	if ( here.currentMain != here.lastMain ) { // navigation has changed
		var hasTab = false;
		for(i=0;i<here.mainNav.length;i++) {
			if(here.currentMain == here.mainNav[i]) {
				hasTab = true;
				break;
			}
		}
		var getItVisited = checkIfVisited('get_it');
		if(!hasTab) {
			// main nav item doesn't have a tab so all tabs off
			if(!getItVisited) {
				document.images["get_it"].src = here.tabMainPath + "get_it_off.gif";
			} else {
				document.images["get_it"].src = here.tabMainPath + "get_it_visited.gif";
			}
			document.images["customize_it"].src = here.tabMainPath + "customize_it_visited.gif";
			return;
		}// chosen image has a tab, so set it and other tab states
		if(document.images[here.lastMain]) {
			var oldMainTab = document.images[here.lastMain];

			if(oldMainTab == "get_it" && !getItVisited)	{
				oldMainTab.src = here.tabMainPath + here.lastMain + "_off.gif";
			} else {
				oldMainTab.src = here.tabMainPath + here.lastMain + "_visited.gif";
			}
		}
		document.images[here.currentMain].src = here.tabMainPath + here.currentMain + "_at.gif";
	}
}

function updateSubNavHistory(tab) {
	if(tab == "none") return;
	var newTab = true;
	for(var i in here.subNavHistory) {
		if(tab == here.subNavHistory[i]) {
			newTab = false;
			return;
		}
	}
	if(newTab == true) {
		here.subNavHistory.push(tab);
	}
}

function didYouKnowUpdate() {
	if(here.currentSub != "none") {
		document.getElementById("did_you_know").innerHTML = here.didYouKnow[here.currentSub];
	} else {
		document.getElementById("did_you_know").innerHTML = here.didYouKnow[here.currentMain];
	}
}

function mainNavToggle(theImage, state) {

	if(here.currentMain == theImage) {
		return;
	}
	var imgState;
	if(state == "on") {
		imgState =  "_on.gif";
	}
	if(state == "off") {
		if(theImage == "get_it") {
			if(!checkIfVisited('get_it')) {
				imgState = "_mouse_off.gif";
			} else {
				imgState =  "_mouse_visited.gif";
			}
		} else {
			imgState =  "_mouse_visited.gif";
		}
	}
	document.images[theImage].src = here.tabMainPath + theImage + imgState;
}

function tabToggle(theImage, state) {

	var newSource = here.tabPath + theImage;
	if(here.currentSub == theImage) {
		return;
	}
	if(state == "on") {
		newSource = newSource + "_on.gif";
		document.images[theImage].src = newSource;
		return;
	}
	if(state == "off") {
		for(var i in here.subNavHistory){
			if(theImage == here.subNavHistory[i]) {
				newSource = newSource + "_visited.gif";
				document.images[theImage].src = newSource;
				return;
			}
		}
	newSource = newSource + "_off.gif";
	document.images[theImage].src = newSource;
	}
}

function getCookie(name)
    {
        var dc = document.cookie;
        var prefix = name + "=";
        var begin = dc.indexOf("; " + prefix);
        if (begin == -1)
        {
            begin = dc.indexOf(prefix);
            if (begin != 0) return null;
        }
        else
        {
            begin += 2;
        }
        var end = document.cookie.indexOf(";", begin);
        if (end == -1)
        {
            end = dc.length;
        }
        return unescape(dc.substring(begin + prefix.length, end));
}


