function contactDealer(dealerCode) {
	var configForm = document.getElementById("configurationForm");
	var dealerCodeElement = document.getElementById("dealerCodeField");
	if ( dealerCodeElement == null ) {
		dealerCodeElement = document.createElement("input");
		dealerCodeElement.id = "dealerCodeField";
		dealerCodeElement.name = "dealerCode";
		dealerCodeElement.type = "hidden";
		configForm.appendChild(dealerCodeElement);
	}
	dealerCodeElement.value = dealerCode;
	loadFindDealers();
	dealerCodeElement.value = "";
}

function loadDealerMap(dealerCode) {
	var dealerData = new UrlData();
	dealerData.add("width", "388");
	dealerData.add("height", "228");
	dealerData.add("dealerCode", dealerCode);
	var newLocation = '/BysDev2/pub/dealerMap.do' + '?' + dealerData.cgiString;
	loadFindDealers(newLocation);
}




// global flag
	var isIE = false;
	// global request and XML document objects
	var req;
	// current content container
	var container;

	var onLoadDocSuccess = null;

	function loadDoc(url, method, data, onSuccess) {
		here.hold = true;
		if ( typeof(data) == "undefined" ) data = null;
		if ( ( method == "GET" ) && ( data != null ) ) {
			url += "?" + data;
		}
		// branch for native XMLHttpRequest object
		if (window.XMLHttpRequest) {
			req = new XMLHttpRequest();
			req.onreadystatechange = processReqChange;
			req.open(method, url, true);
			if (method == "POST") {
				var post = data;
				if ( post == null ) {
					post = "";
				}
				post += configurationPostString();
				req.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
				req.setRequestHeader('BYS-Request-Type', 'page-component');
				req.send(post);
			}
			else {
				req.send(null);
			}
		// branch for IE/Windows ActiveX version
		} else if (window.ActiveXObject) {
			isIE = true;
			req = new ActiveXObject("Microsoft.XMLHTTP");
			if (req) {
				req.onreadystatechange = processReqChange;
				req.open(method, url, true);
				if(method == "POST") {
					var post = data;
					if ( post == null ) {
						post = "";
					}
					post = configurationPostString();
					req.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
					req.setRequestHeader('BYS-Request-Type', 'page-component');
					req.send(post);
				} else {
					req.send();
				}
			}
		}
		if ( typeof(onSuccess) != "undefined" ) {
			onLoadDocSuccess = onSuccess;
		}
		else {
			onLoadDocSuccess = null;
		}
	}

	function UrlData() {
		function urlDataAdd(key, value) {
			if ( this.cgiString != "" ) this.cgiString += "&";
			this.cgiString += key + "=" + escape(value);
		}

		this.cgiString = "";
		this.add = urlDataAdd;

		return this;
	}

	function configurationPostString() {
		var data = new UrlData();
		var selectedColor = "";
		for (var i in colorMap) {
			var color = colorMap[i];
			if ( color.selected ) {
				selectedColor=color.id;
				break;
			}
		}
		if ( selectedColor != "" ) data.add("colorId", selectedColor);
		for (var i in accessoryMap) {
			var accessory = accessoryMap[i];
			if ( accessory.selected ) data.add("accessories", accessory.id);
		}
		return data.cgiString;
	}

	function processReqChange() {
		var displayArea =  document.getElementById(container);
		var loading =  document.getElementById("loading");
		displayArea.style.display="none";
		loading.style.display="block";
		if (req.readyState == 4) {
			here.hold = false;
			if (req.status == 200) {
				loading.style.display="none";
				var newContent = req.responseText;
				displayArea.style.display="block";
				displayArea.innerHTML = newContent;
				if ( onLoadDocSuccess != null ) {
					onLoadDocSuccess();
					onLoadDocSuccess = null;
				}
			} else {
				loading.style.display="none";
				document.getElementById('errorArea').style.display = "block";
				//alert("There was a problem retrieving the requested data:\n" + req.status + req.statusText);
			}
		}
	}



