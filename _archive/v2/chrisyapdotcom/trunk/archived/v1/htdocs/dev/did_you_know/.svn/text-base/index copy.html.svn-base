function loadGetIt() {
	if(here.currentMain != "get_it" && here.hold != true) {
		hideContentAreas();
		var visited = checkIfVisited('get_it');
		if(!visited) {
			container='getItArea';
			loadDoc('getIt.do', 'POST');
		}
		updateLocation('get_it','none');
		document.getElementById("getItArea").style.display="block";
		updatePageTitle('Build Your Scion | Get It');
		omniture('sci: bys: get it');

	}
	return false;
}
function loadCustomizeIt() {
	if(here.currentMain != "customize_it" && here.hold != true) {
		hideContentAreas();
		document.getElementById("contentArea").style.display="block";
		document.Standard_features.src = document.Standard_features.src
		if(here.currentSub == "none" && here.lastSub == "Standard_features") {
			loadStandardFeatures();
			return;
		}
		updatePageTitle('Build Your Scion | Customize It');
		updateLocation('customize_it',here.currentSub);

		omniture('sci: bys: customize_it');
		setTimeout("loadSubnavSrc()",200);
	}
}
