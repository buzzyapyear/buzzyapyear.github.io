	function AccessoryGroup( id, name, viewpointKey, isExclusive, category, showName ) {

		function addAccessoryToGroup( accessory ) {
			this.accessoryList.push(accessory);
		}

		this.id = id;
		this.name = name;
		this.viewpointKey = viewpointKey;
		this.isExclusive = isExclusive;
		this.category = category;
		this.showName = showName;

		this.accessoryList = new Array();

		this.addAccessory = addAccessoryToGroup;

		return this;
	}

	function Accessory( id, name, msrp, excludeIds, accessoryGroup, viewpointKey, description, disclaimer, imageUrl ) {
		this.id = id;
		this.name = name;
		this.msrp = msrp;
		this.excludeIds = excludeIds;
		this.group = accessoryGroup;
		this.viewpointKey = viewpointKey;
		this.description = description;
		this.disclaimer = disclaimer;
		this.imageUrl = imageUrl;
		this.checkbox = null;
		this.excluded = false;
		this.selected = false;

		accessoryGroup.addAccessory(this);

		return this;
	}


	

