<script type="text/javascript">
	function resize() {
	var currentWidth;
	var currentHeight;
	var desiredWidth = 350;
	var desiredHeight = 350;
	if (parseInt(navigator.appVersion)>3) {
		if (navigator.appName=="Netscape") {
			currentWidth = window.innerWidth-16;
			currentHeight = window.innerHeight-16;
		}
		if (navigator.appName.indexOf("Microsoft")!=-1) {
			currentWidth = document.body.offsetWidth-20;
			currentHeight = document.body.offsetHeight-20;
		}
	}
	this.resizeTo(desiredWidth,desiredHeight);
	this.moveTo(0,0);
}

</script>
