body {
	width: 300px;
	background-color: #FFFFFF;
	margin: 0;
	padding: 5px;
	font-family: Arial, Helvetica, sans-serif;
}

div.clearer {
	clear: both;
}

div#loading_div {
	width: 300px;
	height: 300px;
	background-color: #FFFFFF;
	padding: 80px 0 0 120px;
	position: absolute;
	z-index: 5000;
}

div#contact_list_wrapper {
	visibility: hidden;
}

div#contact_list_wrapper div#header {
	width: 300px;
	height: 31px;
	background: url(/goog/images/bg_top.gif) top left no-repeat;
}

div#contact_list_wrapper div#header h1 {
	font-size: 1em;
	padding: 7px 0 0 10px;
	margin: 0;
}

div#contact_list_wrapper div#list_wrapper {
	border-left: 1px solid #335ea8;
	border-right: 1px solid #335ea8;
	margin: 0 !important;
	padding: 10px 0 10px 0;
	font-size: .8em;
}

div#contact_list_wrapper div#list_wrapper ul#contact_list {
	margin: 0;
	padding: 0;
	list-style-type: none;
	position: relative;
}

div#contact_list_wrapper div#list_wrapper ul#contact_list li {
	position: relative;
	z-index: 100;
}

div#contact_list_wrapper div#list_wrapper ul#contact_list li.hover {
	z-index: 1000 !important;
}

div#contact_list_wrapper div#list_wrapper ul#contact_list  li div.contact_info_name {
	width: 77px;
	padding: 3px 3px 3px 10px;
	position: relative;
	border-top: 1px solid #ffffff;
	border-bottom: 1px solid #ffffff;
}

div#contact_list_wrapper div#list_wrapper ul.backdrop  div.contact_info_name {
	opacity: .50;
	filter:alpha(opacity=50);
}

div#contact_list_wrapper div#list_wrapper ul#contact_list  li.hover div.contact_info_name {
	background-color: #e7e7e7;
	opacity: 1;
	z-index:200;
	filter:alpha(opacity=100);
	border-top: 1px solid #666666;
	border-bottom: 1px solid #666666;
}

div#contact_list_wrapper div#list_wrapper ul#contact_list li div.contact_info_short {
	display: block;
	position: absolute;
	left: 90px;
	top: 0px;
	padding: 3px 10px 3px 10px;
}

div#contact_list_wrapper div#list_wrapper ul.backdrop li div.contact_info_short {
	opacity: .50;
	filter:alpha(opacity=50);
}

div#contact_list_wrapper div#list_wrapper ul#contact_list li.hover div.contact_info_short {
	display: none;
	opacity: 1;
	filter:alpha(opacity=100);
}

div#contact_list_wrapper div#list_wrapper ul#contact_list li div.contact_info_short div.phone {
	display: none;
}

div#contact_list_wrapper div#list_wrapper ul#contact_list li div.contact_info_full {
	visibility: hidden;
	position: absolute;
	left: -5000px;
	top: 0px;
	padding: 3px 10px 3px 10px;
	width: 188px;
	height: auto;
}

div#contact_list_wrapper div#list_wrapper ul#contact_list li.hover div.contact_info_full {
	visibility: visible;
	left: 89px;
	background-color: #e7e7e7;
	z-index: 100;
	border-top: 1px solid #666666;
	border-bottom: 1px solid #666666;
	border-left: 1px solid #666666;
}

div#contact_list_wrapper div#list_wrapper ul#contact_list li div.contact_info_full iframe {
	position: absolute;
	left: 0;
	bottom: 0;
	z-index: 1 !important;
	filter:alpha(opacity=0);
}

div#contact_list_wrapper div#list_wrapper ul#contact_list li.hover div.contact_info_full div.contact_info_avatar {
	position: absolute;
	top: 0px;
	right: 0px;
	margin: 5px 5px 0 5px;
}

div#contact_list_wrapper div#footer {
	width: 300px;
	height: 35px;
	background: url(/goog/images/bg_bottom.gif) top left no-repeat;
}

div#contact_list_wrapper div#footer select {
	float: right;
	margin-right: 10px;
}